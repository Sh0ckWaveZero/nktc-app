{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -b swc -w",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "seed": "ts-node ./src/database/prisma/seed.ts",
    "seed2": "ts-node prisma-mongodb/seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev --name nktc-app-db",
    "prisma:push": "npx prisma db push",
    "prisma:generate-mongo": "npx prisma generate --schema ./src/database/prisma-mongodb/schema.prisma",
    "db:push2": "npx prisma db push --schema ./src/database/prisma-mongodb/schema.prisma",
    "postinstall": "npx prisma generate && npx prisma generate --schema ./src/database/prisma-mongodb/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "10.2.8",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "10.2.8",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "10.2.8",
    "@nestjs/swagger": "7.1.15",
    "@nestjs/throttler": "^5.1.0",
    "argon2": "^0.31.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "decode-uri-component": "0.2.2",
    "helmet": "^5.1.1",
    "minio": "^7.1.3",
    "node-xlsx": "^0.23.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.14",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "run-script-webpack-plugin": "^0.0.14",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "ua-parser-js": "^1.0.37",
    "webpack": "^5.89.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.5",
    "@babel/traverse": "7.23.5",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "10.2.10",
    "@prisma/client": "5.6.0",
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.99",
    "@types/babel__core": "7.20.5",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/decode-uri-component": "^0.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "27.5.0",
    "@types/multer": "1.4.11",
    "@types/node": "18.18.9",
    "@types/node-xlsx": "^0.21.0",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "2.0.16",
    "@types/ua-parser-js": "0.7.39",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.10.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.7.0",
    "prettier": "^2.8.8",
    "prisma": "5.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "schema": "./src/database/prisma/schema.prisma"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  }
}