{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -b swc -w",
    "dev": "nest start -b swc -w",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "seed": "ts-node ./src/database/prisma/seed.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:query": "ts-node ./src/scripts/query-db.ts",
    "db:test-lookups": "ts-node ./src/scripts/test-id-lookups.ts",
    "db:fix-ids": "ts-node ./src/scripts/fix-undefined-ids.ts",
    "db:fix-programs": "ts-node ./src/scripts/fix-program-connections.ts",
    "db:redistribute": "ts-node ./src/scripts/redistribute-students.ts",
    "db:fix-remaining": "ts-node ./src/scripts/fix-remaining-programs.ts",
    "db:connect-classrooms": "ts-node ./src/scripts/connect-classrooms-programs.ts",
    "db:enhanced-import": "ts-node ./src/scripts/enhanced-student-import.ts",
    "prisma:generate": "bun prisma generate --schema ./src/database/prisma/schema.prisma",
    "prisma:migrate": "bun prisma migrate dev --name nktc-app-db",
    "prisma:push": "bun prisma db push",
    "postinstall": "bun run prisma:generate"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.19.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "decode-uri-component": "0.4.1",
    "helmet": "^8.1.0",
    "minio": "^8.0.6",
    "multer": "^2.0.2",
    "node-xlsx": "^0.24.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rimraf": "^6.1.0",
    "run-script-webpack-plugin": "^0.2.3",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.6",
    "uuid": "^13.0.0",
    "webpack": "^5.102.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@swc-node/register": "^1.11.1",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.0",
    "@swc/jest": "^0.2.39",
    "@types/babel__core": "^7.20.5",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/decode-uri-component": "^0.2.2",
    "@types/express": "^5.0.5",
    "@types/jest": "30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/node-xlsx": "^0.21.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.3",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "30.2.0",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@apis/(.*)$": "<rootDir>/apis/$1",
      "^@common/(.*)$": "<rootDir>/common/$1",
      "^@config/(.*)$": "<rootDir>/config/$1",
      "^@database/(.*)$": "<rootDir>/database/$1",
      "^@utils/(.*)$": "<rootDir>/utils/$1",
      "^@lib/(.*)$": "<rootDir>/lib/$1",
      "^@middlewares/(.*)$": "<rootDir>/middlewares/$1"
    }
  },
  "prisma": {
    "schema": "./src/database/prisma/schema.prisma"
  },
  "resolutions": {
    "tmp": "^0.2.5",
    "validator": "^13.15.15"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint",
      "options": {}
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {}
    }
  }
}
