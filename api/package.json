    "add": "^2.0.6",
    "dayjs": "^1.11.13",
    "drizzle-orm": "^0.33.0",
    "elysia": "1.1.22",
    "pg": "^8.13.1",
    "pino-pretty": "^13.0.0",
    "pino-roll": "^2.2.0",
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
