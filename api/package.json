  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "format": "prettier . --write --config prettier.config.cjs",
    "format:check": "prettier . --check",
